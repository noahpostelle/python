def main(lst):
   
    if len(lst) == 1:
        return lst[0]
    
    remainder = main(lst[1:])

    if lst[0] > remainder:
        return lst[0]
    else:
        return remainder

list = [3, 7, 15, 12, 5, 9, 200]
largest = main(list)
print("The largest value in the list is:", largest)


